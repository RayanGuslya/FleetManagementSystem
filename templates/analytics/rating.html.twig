{% extends 'base.html.twig' %}
{% block body %}
<h1>Рейтинг автомобилей по затратам</h1>

<table border="1" cellpadding="6">
    <tr>
        <th>ID авто</th>
        <th>Модель</th>
        <th>Затраты всего</th>
    </tr>

    {% for row in data %}
        <tr>
            <td>{{ row.id }}</td>
            <td>{{ row.model }}</td>
            <td>{{ row.total_costs }}</td>
        </tr>
    {% endfor %}
</table>
{% endblock %}
